package com.sogou.bizwork.task.api.core.task.po;

public class Scheduled {
	private int periodFrequency;
	private int periodSpecific;
	private int periodValid;
	
	
	public int getPeriodFrequency() {
		return periodFrequency;
	}
	public void setPeriodFrequency(int periodFrequency) {
		this.periodFrequency = periodFrequency;
	}
	public int getPeriodSpecific() {
		return periodSpecific;
	}
	public void setPeriodSpecific(int periodSpecific) {
		this.periodSpecific = periodSpecific;
	}
	public int getPeriodValid() {
		return periodValid;
	}
	public void setPeriodValid(int periodValid) {
		this.periodValid = periodValid;
	}
	
}
