package com.sogou.bizwork.task.api.core.user.dto;

public class GroupInfoDto {
	private String groupname;
	private String deptIntro;
	private String groupIntro;
	private String specialInfo;
	private String techStandard;
	private String softwareEnv;
	
	public String getGroupname() {
		return groupname;
	}
	public void setGroupname(String groupname) {
		this.groupname = groupname;
	}
	public String getDeptIntro() {
		return deptIntro;
	}
	public void setDeptIntro(String deptIntro) {
		this.deptIntro = deptIntro;
	}
	public String getGroupIntro() {
		return groupIntro;
	}
	public void setGroupIntro(String groupIntro) {
		this.groupIntro = groupIntro;
	}
	public String getSpecialInfo() {
		return specialInfo;
	}
	public void setSpecialInfo(String specialInfo) {
		this.specialInfo = specialInfo;
	}
	public String getTechStandard() {
		return techStandard;
	}
	public void setTechStandard(String techStandard) {
		this.techStandard = techStandard;
	}
	public String getSoftwareEnv() {
		return softwareEnv;
	}
	public void setSoftwareEnv(String softwareEnv) {
		this.softwareEnv = softwareEnv;
	}
	
}
